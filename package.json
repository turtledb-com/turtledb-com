  "bin": "./bin/turtledb-com.js",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.782.0",
    "@gerhobbelt/gitignore-parser": "^0.2.0-9",
    "@noble/secp256k1": "^2.2.3",
